pipeline {
    agent any

    stages {
        stage('Git Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/DevopsPractice95/Project5.git'
            }
        }
        
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        
        stage('Helo') {
            steps {
                echo 'Hello World'
            }
        }
    }
}
